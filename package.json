{
    "name": "@vigilio/express",
    "version": "0.0.1",
    "private": true,
    "description": "create web apps with @vigilio/express now",
    "scripts": {
        "dev": "vite",
        "serve": "cross-env NODE_ENV=development  ts-node-dev --poll --respawn -r  tsconfig-paths/register   app/main.ts",
        "build:serve": "rimraf dist && tsc  --project tsconfig.build.json ",
        "build:dev": "tsc --project ./resources/tsconfig.json && vite build ",
        "start:dev": "concurrently \"pnpm dev\" \"pnpm serve\"",
        "start:prod": "cross-env NODE_ENV=production node dist/main.js",
        "test": "vitest",
        "biome:format:server": "biome format  ./app --write",
        "biome:check:server": "biome check ./app",
        "biome:format:client": "biome format  ./resources/ts --write",
        "biome:check:client": "biome check ./resources/ts",
        "biome:format": "pnpm biome:format:server && pnpm biome:format:client",
        "biome:check": "pnpm biome:check:server && pnpm biome:check:client"
    },
    "keywords": [
        "@vigilio/express",
        "@vigilio/express-api"
    ],
    "author": "Vigilio",
    "license": "ISC",
    "dependencies": {
        "@decorators/di": "^2.0.0",
        "@decorators/express": "^2.9.4",
        "@preact/signals": "^1.2.2",
        "@vigilio/express-core": "^1.7.3",
        "@vigilio/preact-fetching": "^2.0.7",
        "@vigilio/preact-paginator": "^0.0.8",
        "@vigilio/preact-table": "^1.0.16",
        "@vigilio/sweet": "^1.0.11",
        "alpinejs": "^3.13.3",
        "aos": "^2.3.4",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cross-env": "^7.0.3",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "formidable": "^3.5.1",
        "js-cookie": "^3.0.5",
        "memorystore": "^1.6.7",
        "million": "^2.6.4",
        "module-alias": "^2.2.3",
        "morgan": "^1.10.0",
        "passport": "^0.7.0",
        "passport-local": "^1.0.0",
        "pg": "^8.11.3",
        "preact": "^10.19.2",
        "react-hook-form": "^7.48.2",
        "react-slick": "^0.29.0",
        "sequelize": "^6.35.1",
        "sequelize-typescript": "^2.1.6",
        "sharp": "0.32.6",
        "tslib": "^2.6.2",
        "valibot": "^0.20.1",
        "wouter-preact": "^2.12.2"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.4.1",
        "@faker-js/faker": "^8.3.1",
        "@preact/preset-vite": "^2.7.0",
        "@types/alpinejs": "^3.13.5",
        "@types/aos": "^3.0.7",
        "@types/bcryptjs": "^2.4.6",
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.6",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.17.10",
        "@types/formidable": "^3.4.5",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/module-alias": "^2.0.4",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.10.4",
        "@types/passport": "^1.0.16",
        "@types/react": "^18.2.41",
        "@types/supertest": "^2.0.16",
        "autoprefixer": "^10.4.16",
        "concurrently": "^8.2.2",
        "postcss": "^8.4.32",
        "rimraf": "^5.0.5",
        "supertest": "^6.3.3",
        "tailwindcss": "^3.3.6",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3",
        "unplugin-swc": "^1.4.4",
        "vite": "^5.0.4",
        "vite-plugin-live-reload": "^3.0.3",
        "vitest": "^0.34.6"
    }
}
